# Environment variables for CKKS Credit Scoring System

# Backend Configuration
GO_ENV=production
BACKEND_PORT=8080

# Frontend Configuration
NODE_ENV=production
REACT_APP_API_URL=http://localhost:8080

# Docker Configuration
COMPOSE_PROJECT_NAME=ckks_credit

# Logging
LOG_LEVEL=info

# Security (for production)
# TLS_CERT_PATH=/path/to/cert.pem
# TLS_KEY_PATH=/path/to/key.pem

# Performance
# GOMAXPROCS=4
# WORKER_THREADS=4
